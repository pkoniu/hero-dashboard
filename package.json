{
  "name": "hero-dashboard",
  "version": "1.0.0",
  "description": "Dashboard with apps deployed on heroku",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node bin/www",
    "test": "./node_modules/mocha/bin/mocha test/*.js",
    "infra": "node infra/hero-dash-infra.js",
    "jshint": "jshint src/*.js test/*.js",
    "build:less": "lessc public/less/styles.less public/styles.css",
    "build": "npm run build:less",
    "build:watch": "watch 'npm run build:less' .",
    "build-and-test": "npm run build && npm test && npm run jshint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkoniu/hero-dashboard.git"
  },
  "keywords": [
    "heroku",
    "dashboard",
    "support"
  ],
  "authors": [
    "Patryk Konior",
    "Mateusz Kmiecik",
    "Patryk ≈Åawski"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pkoniu/hero-dashboard/issues"
  },
  "homepage": "https://github.com/pkoniu/hero-dashboard#readme",
  "dependencies": {
    "body-parser": "^1.15.0",
    "cheerio": "^0.20.0",
    "cookie-parser": "1.4.1",
    "debug": "^2.2.0",
    "express": "4.13.4",
    "heroin-js": "^1.12.1",
    "heroku-client": "^2.4.3",
    "jshint": "^2.9.2",
    "less": "^2.7.1",
    "lodash": "^4.11.2",
    "mocha": "^2.4.5",
    "moment": "^2.13.0",
    "morgan": "1.7.0",
    "run-sequence": "^1.2.0",
    "supertest": "^1.2.0",
    "watch": "^0.18.0"
  }
}
